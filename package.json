{
  "name": "ecpb",
  "version": "1.0.0",
  "author": "Valerie McNett",
  "description": "Simple Gatsby.js starter that uses material design and that's perfect for tiny agencies.",
  "scripts": {
    "develop": "gatsby develop",
    "start": "npm run develop",
    "dev-css": "lessc ./src/components/less/main.less > ./src/components/dist/main.css",
    "post-css": "postcss --use autoprefixer -b 'last 2 versions' < ./src/components/dist/main.css | cssmin > ./src/components/dist/main.min.css ",
    "dist-css": "npm run dev-css && npm run post-css",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public"
  },
  "devDependencies": {
    "prettier": "^1.15.3",
    "css-loader": "^1.0.0",
    "cssmin": "^0.4.3",
    "postcss": "^7.0.14",
    "postcss-cli": "^4.1.1"
  },
  "dependencies": {
    "@material-ui/core": "^3.9.2",
    "core-js": "^2.6.5",
    "eslint": "^5.15.1",
    "gatsby": "^2.1.15",
    "gatsby-image": "^2.0.29",
    "gatsby-link": "^2.0.12",
    "gatsby-plugin-favicon": "^3.1.5",
    "gatsby-plugin-google-analytics": "^2.0.14",
    "gatsby-plugin-less": "^2.0.11",
    "gatsby-plugin-manifest": "^2.0.19",
    "gatsby-plugin-offline": "^2.0.24",
    "gatsby-plugin-react-helmet": "^3.0.6",
    "gatsby-plugin-remove-serviceworker": "^1.0.0",
    "gatsby-plugin-sharp": "^2.0.22",
    "gatsby-source-filesystem": "^2.0.22",
    "gatsby-source-instagram": "^0.2.5",
    "gatsby-transformer-remark": "^2.2.5",
    "gatsby-transformer-sharp": "^2.1.14",
    "gh-pages": "^2.0.1",
    "jss": "^9.8.7",
    "less": "^3.9.0",
    "mdi-material-ui": "^5.9.0",
    "postcss": "^7.0.14",
    "postcss-cli": "^4.1.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "react-helmet": "^5.2.0",
    "react-jss": "^8.6.1",
    "react-slick": "^0.23.2",
    "react-swipeable-views": "^0.13.1",
    "react-swipeable-views-utils": "^0.13.1",
    "slick-carousel": "^1.8.1",
    "styled-components": "^4.1.3"
  }
}
